{
    "name": "sycms/text-editor-bundle",
    "description": "Text Editor SyCms",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/framework-bundle": "^2.8",
        "symfony-cmf/resource-bundle": "@dev",
        "puli/repository": "@beta"
    },
    "require-dev": {
        "symfony/console": "^2.8",
        "symfony/yaml": "^2.8",
        "symfony/process": "^2.8",
        "phpunit/phpunit": "@stable",
        "symfony/twig-bundle": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Sycms\\Bundle\\TextEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sycms\\Bundle\\TextEditor\\Tests\\": "tests/",
            "Sycms\\Bundle\\TextEditor\\Example\\": "example/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
